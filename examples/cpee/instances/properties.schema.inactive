<properties xmlns="http://riddl.org/ns/common-patterns/properties/1.0">
  <name type="value" modifiable="false" mutable="false">
    <text/>
  </name>
  <handler type="value" modifiable="true">
    <data type="string">
      <param name="pattern">[A-Z][a-zA-Z]*</param>
    </data>  
  </handler>
  <dsl type="value" modifiable="false" mutable="false">
    <text/>
  </dsl>
  <description type="content" modifiable="true"/>
  <positions type="list" modifiable="false" mutable="false"/>
  <optional>
    <transformation type="content" modifiable="true"/>
  </optional>  
  <state type="state" modifiable="true">
    <ready>
      <running putable="true"/>
    </ready>  
    <running>
      <stopping putable="true"/>
      <finished putable="false"/>
    </running>
    <stopping>
      <stopped putable="false"/>
    </stopping>
    <stopped>
      <running putable="true"/>
    </stopped>  
    <finished/>
  </state>  
  <context-variables type="map" modifiable="true"/>
  <endpoints type="map" modifiable="true"/>
</properties>
