<testset>
  <positions xmlns="http://cpee.org/ns/properties/2.0"/>
  <handlerwrapper xmlns="http://cpee.org/ns/properties/2.0">DefaultHandlerWrapper</handlerwrapper>
  <dataelements xmlns="http://cpee.org/ns/properties/2.0">
    <rev>1</rev>
  </dataelements>
  <endpoints xmlns="http://cpee.org/ns/properties/2.0">
    <timeout>http://gruppe.wst.univie.ac.at/~mangler/services/timeout.php</timeout>
  </endpoints>
  <description xmlns="http://cpee.org/ns/properties/2.0">
    <description xmlns="http://cpee.org/ns/description/1.0">
      <parallel>
        <parallel_branch>
          <loop mode="pre_test" condition="data.feedback">
            <call id="publish" endpoint="timeout">
              <parameters>
                <!--{{{-->
                <method>:post</method>
                <arguments>
                  <timeout>1</timeout>
                </arguments>
              </parameters>
              <!--}}}-->
            </call>
            <choose>
              <alternative condition="data.x != nil">
                <call id="a4_1" endpoint="timeout">
                  <parameters>
                    <!--{{{-->
                    <method>:post</method>
                    <arguments>
                      <timeout>4</timeout>
                    </arguments>
                  </parameters>
                  <!--}}}-->
                </call>
              </alternative>
              <otherwise>
                <call id="a4_2" endpoint="timeout">
                  <parameters>
                    <!--{{{-->
                    <method>:post</method>
                    <arguments>
                      <timeout>10</timeout>
                    </arguments>
                  </parameters>
                  <!--}}}-->
                </call>
              </otherwise>
            </choose>
            <choose>
              <alternative condition="data.x != nil">
                <call id="a4_1" endpoint="timeout">
                  <parameters>
                    <!--{{{-->
                    <method>:post</method>
                    <arguments>
                      <timeout>4</timeout>
                    </arguments>
                  </parameters>
                  <!--}}}-->
                </call>
              </alternative>
              <otherwise>
                <call id="a4_2" endpoint="timeout">
                  <parameters>
                    <!--{{{-->
                    <method>:post</method>
                    <arguments>
                      <timeout>10</timeout>
                    </arguments>
                  </parameters>
                  <!--}}}-->
                </call>
              </otherwise>
            </choose>
          </loop>
          <call id="a4_1" endpoint="timeout">
            <parameters>
              <!--{{{-->
              <method>:post</method>
              <arguments>
                <timeout>4</timeout>
              </arguments>
            </parameters>
            <!--}}}-->
          </call>
        </parallel_branch>
        <parallel_branch>
          <loop mode="pre_test" condition="data.feedback">
            <call id="prepare" endpoint="timeout">
              <parameters>
                <!--{{{-->
                <method>:post</method>
                <arguments>
                  <timeout>4</timeout>
                </arguments>
              </parameters>
              <!--}}}-->
            </call>
            <parallel>
              <choose>
                <parallel_branch>
                  <alternative condition="data.x != nil">
                    <call id="a4_1" endpoint="timeout">
                      <parameters>
                        <!--{{{-->
                        <method>:post</method>
                        <arguments>
                          <timeout>4</timeout>
                        </arguments>
                      </parameters>
                      <!--}}}-->
                    </call>
                  </alternative>
                </parallel_branch>
                <parallel_branch>
                  <alternative condition="data.x.nil?">
                    <call id="a4_2" endpoint="timeout">
                      <parameters>
                        <!--{{{-->
                        <method>:post</method>
                        <arguments>
                          <timeout>10</timeout>
                        </arguments>
                      </parameters>
                      <!--}}}-->
                    </call>
                  </alternative>
                </parallel_branch>
              </choose>
              <choose>
                <parallel_branch>
                  <alternative condition="data.x != nil">
                    <call id="a4_1" endpoint="timeout">
                      <parameters>
                        <!--{{{-->
                        <method>:post</method>
                        <arguments>
                          <timeout>4</timeout>
                        </arguments>
                      </parameters>
                      <!--}}}-->
                    </call>
                  </alternative>
                </parallel_branch>
                <parallel_branch>
                  <alternative condition="data.x.nil?">
                    <call id="a4_2" endpoint="timeout">
                      <parameters>
                        <!--{{{-->
                        <method>:post</method>
                        <arguments>
                          <timeout>10</timeout>
                        </arguments>
                      </parameters>
                      <!--}}}-->
                    </call>
                  </alternative>
                </parallel_branch>
              </choose>
            </parallel>
          </loop>
        </parallel_branch>
      </parallel>
    </description>
  </description>
  <transformation xmlns="http://cpee.org/ns/properties/2.0">
    <description type="copy"/>
    <dataelements type="rest"/>
    <endpoints type="rest"/>
  </transformation>
</testset>